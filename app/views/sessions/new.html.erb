<% provide(:title, "Sign in") %>
<script>
$(document).ready(function(){
  $("#session_name").focus();
});
</script>


<div class="ssfood_form">
<h1>Log in</h1>
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :password %>
      <%= f.password_field :password %>
      <%= f.submit "Log in", class: "btn btn-large btn-primary" %>
    <% end %>
</div>